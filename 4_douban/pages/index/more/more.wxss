/* pages/index/more/more.wxss */
.moreAll{
  padding: 15px;
  overflow: hidden;
}
.moreOne{
  float: left;
  width: 85px;
}
.moreAll>view:nth-child(3n+2){
  margin: 0 15px;
}
.moreImg{
  width: 100%;
  height: 110px;
}
.moreTitle{
  display: block;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 13px;
  font-weight: 600;
}
.moreRate{
  display: block;
  font-size: 11px;
  color: gray;
}